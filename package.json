{
  "name": "omsorgspenger-visning-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "@navikt/fnrvalidator": "1.1.4",
    "@navikt/nap-header": "1.0.10",
    "@navikt/nap-person-card": "1.1.4",
